fresh zsh/aliases/init --marker --file=~/.zshrc

fresh zsh/aliases/k8s --marker --file=~/.zshrc

fresh zsh/aliases/terminal --marker --file=~/.zshrc

fresh zsh/aliases/terraform --marker --file=~/.zshrc

fresh zsh/zplug/init --marker --file=~/.zshrc

fresh zsh/zplug/plugins --marker --file=~/.zshrc

fresh zsh/zplug/install --marker --file=~/.zshrc

fresh zsh/functions/include --marker --file=~/.zshrc
